{
  "presets": ["env"],
  "plugins": [
    "transform-class-properties", "transform-object-rest-spread",
    "syntax-object-rest-spread"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}